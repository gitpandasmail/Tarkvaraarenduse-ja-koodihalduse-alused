# Lisa siia vastuste ja lahenduste kood
### 1. VECTOR
nimed_bio <- c("Anna", "Mike", "Luis")
vanused_bio <- c(20, 21, 19)
tulemused_bio <- c(85, 78, 90)
### 2. MATRIX
vanused <- c(20, 21, 19, 22, 20, 23, 24, 22, 25)
tulemused <- c(85, 78, 90, 88, 76, 95, 87, 80, 92)
# Loo maatriks (2 veergu: vanus, tulemus)
andmed_matrix <- matrix(c(vanused, tulemused), ncol = 2)
colnames(andmed_matrix) <- c("Vanus", "Tulemus")
rownames(andmed_matrix) <- c("Bio1", "Bio2", "Bio3", "Keem1", "Keem2", "Keem3", "Füüs1", "Füüs2", "Füüs3")
### 3. DATA.FRAME
nimed <- c("Anna", "Mike", "Luis", "Sam", "Ella", "Ravi", "Mia", "Jack", "Olivia")
klassid <- c("Bioloogia", "Bioloogia", "Bioloogia",
"Keemia", "Keemia", "Keemia",
"Füüsika", "Füüsika", "Füüsika")
andmed_df <- data.frame(
Nimi = nimed,
Vanus = vanused,
Tulemus = tulemused,
Klass = klassid
)
### 4. LIST
anna_list <- list(
Nimi = "Anna",
Vanus = 20,
Tulemus = 85,
Klass = "Bioloogia"
)
### 5. ANALÜÜS
# a) Õpilased, kelle tulemus > 85 (väljastab nime ja klassi)
parimad <- subset(andmed_df, Tulemus > 85, select = c(Nimi, Klass))
print("Õpilased tulemusega > 85:")
print(parimad)
# b) Klasside keskmised tulemused (kasutades maatriksit)
# Jagame maatriksi 3 kaupa klassideks
bio_avg <- mean(andmed_matrix[1:3, "Tulemus"])
keem_avg <- mean(andmed_matrix[4:6, "Tulemus"])
fys_avg <- mean(andmed_matrix[7:9, "Tulemus"])
klasside_keskmised <- c(
Bioloogia = bio_avg,
Keemia = keem_avg,
Füüsika = fys_avg
)
print("Klasside keskmised tulemused:")
print(klasside_keskmised)
